#include <stdio.h>

#include<math.h>

int main()

{

    int a[3][3],b[3][3],i,j;

    printf("enter the matrix elements \n");

    for(i=0;i<3;i++)

    {

        for(j=0;j<3;j++)

            scanf("%d",&a[i][j]);

            }

    printf("the matrix is \n");

    for(i=0;i<3;i++)

    {

        for(j=0;j<3;j++)

            printf("%d",a[i][j]);

        printf("\n");

        }

    printf("printing the lower triangle \n");

    for(i=0;i<3;i++)

    {

        for(j=0;j<3;j++)

            if(i<j)

            {

                printf("0");

                

            }

        else

        {

            printf("%d",a[i][j]);

        }

        printf("\n");

    }

        

        printf("printing the upper triangle \n");

        for(i=0;i<3;i++)

        {

            for(j=0;j<3;j++)

                if(i>j)

                {

                    printf("0");

                    

                }

                else

                {

                    printf("%d",a[i][j]);

                }

            printf("\n");

    }}


*5

#include <stdio.h>
#include<math.h>
int main()
{
    int a[2][2],i,j,c[2][2],b[2][2];
    printf("enter the elements of first  array\n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
            scanf("%d",&a[i][j]);
        
    }
    printf("enter the elements of second array\n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
           scanf("%d",&b[i][j]);
        
    }
    printf("\n");
    printf("the product of both the array is \n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
            c[i][j]=a[i][j]*b[i][j];
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
            printf("%d",c[i][j]);
        printf("\n");
            
    }
    printf("\n");
}
    
   



*6

#include <stdio.h>

#include<math.h>

int main()

{

    int a[2][2],i,j,c[2][2],b[2][2],s[2][2];

    printf("enter the elements of first  array\n");

    for(i=0;i<2;i++)

    {

        for(j=0;j<2;j++)

            scanf("%d",&a[i][j]);

        

    }

    printf("enter the elements of second array\n");

    for(i=0;i<2;i++)

    {

        for(j=0;j<2;j++)

           scanf("%d",&b[i][j]);

        

    }

    printf("\n");

    printf("the sum is \n");

    for(i=0;i<2;i++)

    {

        for(j=0;j<2;j++)

            c[i][j]=a[i][j]+b[i][j];

    }

    for(i=0;i<2;i++)

    {

        for(j=0;j<2;j++)

            printf("%d",c[i][j]);

        printf("\n");

            

    }

    printf("\n");

    

    printf("subraction is \n");

    for(i=0;i<2;i++)

    {

        for(j=0;j<2;j++)

            s[i][j]=a[i][j]-b[i][j];

    }

    for(i=0;i<2;i++)

    {

        for(j=0;j<2;j++)

            printf("%d",s[i][j]);

        printf("\n");

        

    }

    printf("\n");

}


*7


#include <stdio.h>
#include<math.h>
int main()
{
    int a[2][2],i,j,t[2][2];
    printf("enter the elements of array\n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
            scanf("%d",&a[i][j]);
        
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
            printf("%d",a[i][j]);
        printf("\n");
        
    }
    printf("\n");
    printf("the transpose \n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
            t[j][i]=a[j][i];
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
            printf("%d",t[j][i]);
        printf("\n");
    }
    printf("\n");
    
   
   
}


